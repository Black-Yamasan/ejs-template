{
  "name": "ejs_template",
  "version": "13.0.0",
  "description": "ejsでのテンプレート",
  "main": "index.js",
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/preset-env": "7.7.7",
    "babel-loader": "8.2.3",
    "browser-sync": "2.27.9",
    "css-loader": "6.7.1",
    "del": "5.1.0",
    "glob": "8.0.3",
    "graceful-fs": "4.2.0",
    "gulp": "4.0.2",
    "gulp-ejs": "5.1.0",
    "gulp-if": "3.0.0",
    "gulp-notify": "4.0.0",
    "gulp-plumber": "1.2.1",
    "gulp-rename": "1.4.0",
    "gulp-watch": "5.0.1",
    "gulp4-run-sequence": "0.2.1",
    "jest": "27.4.7",
    "mini-css-extract-plugin": "2.6.1",
    "node-sass": "7.0.1",
    "postcss-loader": "7.0.0",
    "sass-loader": "13.0.2",
    "terser-webpack-plugin": "5.1.4",
    "webpack": "5.69.1",
    "webpack-remove-empty-scripts": "0.8.0",
    "webpack-stream": "7.0.0"
  },
  "scripts": {
    "build:dev": "npx gulp build --env dev",
    "build:prod": "npx gulp build --env prod",
    "start": "npx gulp",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Black-Yamasan/ejs-template.git"
  },
  "keywords": [
    "npm",
    "install",
    "--save-dev",
    "gulp-plumber"
  ],
  "author": "Black-Yamasan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Black-Yamasan/ejs-template/issues"
  },
  "homepage": "https://github.com/Black-Yamasan/ejs-template#readme",
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.{js}",
      "!**/node_modules/**"
    ]
  }
}
