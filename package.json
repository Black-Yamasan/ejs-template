{
  "name": "ejs_template",
  "version": "14.0.0",
  "description": "ejsでのテンプレート",
  "main": "index.js",
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/preset-env": "7.7.7",
    "babel-loader": "8.2.3",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.1",
    "del": "5.1.0",
    "ejs": "3.1.8",
    "glob": "8.0.3",
    "jest": "27.4.7",
    "mini-css-extract-plugin": "2.6.1",
    "mkdirp": "1.0.4",
    "node-sass": "7.0.3",
    "postcss-loader": "7.0.0",
    "sass-loader": "13.0.2",
    "terser-webpack-plugin": "5.1.4",
    "webpack": "5.69.1",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.10.0",
    "webpack-remove-empty-scripts": "0.8.0",
    "webpack-stream": "7.0.0"
  },
  "scripts": {
    "build:dev": "NODE_ENV=development npx webpack --target node",
    "build:prod": "NODE_ENV=production npx webpack --target node",
    "start": "NODE_ENV=development npx webpack serve --target web",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Black-Yamasan/ejs-template.git"
  },
  "keywords": [
    "npm",
    "yarn",
    "install",
    "--save-dev",
    "webpack"
  ],
  "author": "Black-Yamasan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Black-Yamasan/ejs-template/issues"
  },
  "homepage": "https://github.com/Black-Yamasan/ejs-template#readme",
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.{js}",
      "!**/node_modules/**"
    ]
  }
}
