{
  "name": "ejs_template",
  "version": "14.1.1",
  "description": "ejsでのテンプレート",
  "main": "index.js",
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "volta": {
    "node": "20.9.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/preset-env": "7.24.8",
    "babel-loader": "9.1.3",
    "copy-webpack-plugin": "12.0.2",
    "css-loader": "7.1.2",
    "ejs": "3.1.10",
    "glob": "10.4.5",
    "jest": "29.7.0",
    "mini-css-extract-plugin": "2.9.0",
    "mkdirp": "3.0.1",
    "postcss-loader": "8.1.1",
    "sass": "1.77.8",
    "sass-loader": "14.2.1",
    "sass-migrator": "2.0.3",
    "terser-webpack-plugin": "5.3.10",
    "webpack": "5.93.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.0.4",
    "webpack-remove-empty-scripts": "1.0.4",
    "webpack-stream": "7.0.0"
  },
  "scripts": {
    "build:dev": "NODE_ENV=development npx webpack --target node",
    "build:prod": "NODE_ENV=production npx webpack --target node",
    "start": "NODE_ENV=development npx webpack serve --target web",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Black-Yamasan/ejs-template.git"
  },
  "keywords": [
    "npm",
    "yarn",
    "install",
    "--save-dev",
    "webpack"
  ],
  "author": "Black-Yamasan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Black-Yamasan/ejs-template/issues"
  },
  "homepage": "https://github.com/Black-Yamasan/ejs-template#readme",
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.{js}",
      "!**/node_modules/**"
    ]
  }
}
